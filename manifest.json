{
  "manifest_version": 2,

  "name": "FlatClub",
  "description": "FlatClub filler",
  "version": "1.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "css": [ "style.css" ]
    }
  ],
  "permissions": [
    "activeTab",
    "*://goppa.benivo.com/*",
    "*://flat-club.com/*"
  ]
}